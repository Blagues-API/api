{
  "name": "blagues-api",
  "version": "1.0.0",
  "description": "Blagues API fran√ßaise",
  "main": "index.js",
  "author": "DraftMan",
  "license": "MIT",
  "scripts": {
    "dev": "docker-compose -p blagues-api -f ./docker-compose.dev.yml up",
    "dev:legacy": "nodemon -r dotenv/config --watch '*' --ext 'ts,js,json' --exec ts-node src/index.ts",
    "dev:build": "docker-compose -p blagues-api -f ./docker-compose.dev.yml build",
    "dev:down": "docker-compose -p blagues-api -f ./docker-compose.dev.yml down",
    "prepare": "husky install",
    "lint:nuxt": "eslint --ext .ts,.js,.vue ./src/nuxt",
    "lint:bot": "eslint --ext .ts,.js ./src/bot",
    "lint:fastify": "eslint --ext .ts,.js ./src/api",
    "lint": "npm run lint:nuxt && npm run lint:fastify && npm run lint:bot"
  },
  "lint-staged": {
    "*.{js}": "eslint"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/composition-api": "^0.29.2",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@types/common-tags": "^1.8.1",
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.10.2",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "prisma": "3.1.1",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@nuxtjs/auth": "github:DraftProducts/auth-module#12f4398fa796c354aae0a99e637afd3cdd0ee092",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/svg": "^0.3.0",
    "@prisma/client": "^2.30.3",
    "babel-plugin-prismjs": "^2.1.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "common-tags": "^1.8.0",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "fastify": "^3.22.0",
    "fastify-plugin": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "nuxt": "^2.15.8",
    "prismjs": "^1.25.0",
    "string-similarity": "^4.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blagues-API/blagues-api.git"
  },
  "homepage": "https://www.blagues-api.fr",
  "bugs": {
    "url": "https://github.com/Blagues-API/blagues-api/issues"
  }
}
