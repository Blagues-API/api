{
  "name": "blagues-api",
  "version": "1.0.0",
  "description": "API de blagues fran√ßaises",
  "author": "DraftMan",
  "license": "MIT",
  "scripts": {
    "start": "docker-compose -p blagues_api -f ./docker-compose.yml up -d",
    "down": "docker-compose -p blagues_api -f ./docker-compose.yml down",
    "build": "docker-compose -p blagues_api -f ./docker-compose.yml build",
    "logs": "docker-compose -p blagues_api -f ./docker-compose.yml logs -f app",
    "shell": "docker exec -it blagues_api bash",
    "dev": "docker-compose -p blagues_api_dev -f ./docker-compose.dev.yml up",
    "dev:build": "docker-compose -p blagues_api_dev -f ./docker-compose.dev.yml build",
    "dev:down": "docker-compose -p blagues_api_dev -f ./docker-compose.dev.yml down",
    "dev:shell": "docker exec -it blagues_api_dev bash",
    "prepare": "husky install",
    "lint:nuxt": "eslint --ext .ts,.js,.vue ./src/nuxt",
    "lint:bot": "eslint --ext .ts,.js ./src/bot",
    "lint:fastify": "eslint --ext .ts,.js ./src/api",
    "lint": "npm run lint:nuxt && npm run lint:fastify && npm run lint:bot",
    "dev:migrate": "docker exec -it blagues_api_dev npx prisma migrate dev",
    "dev:generate": "docker exec -it blagues_api_dev npx prisma generate",
    "db:migrate": "docker exec -it blagues_api npx prisma migrate deploy",
    "db:generate": "docker exec -it blagues_api npx prisma generate"
  },
  "lint-staged": {
    "*.{js}": "eslint"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/composition-api": "^0.29.3",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/common-tags": "^1.8.1",
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.7",
    "@types/sharp": "^0.29.3",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "prisma": "3.3.0",
    "sass": "^1.42.1",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@draftbot/nuxtjs-auth": "^5.1.2",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/svg": "^0.3.0",
    "@prisma/client": "^3.4.2",
    "babel-plugin-prismjs": "^2.1.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "common-tags": "^1.8.1",
    "discord.js": "^13.3.0",
    "dotenv": "^10.0.0",
    "fastify": "^3.22.1",
    "fastify-plugin": "^3.0.0",
    "got": "^11.8.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.8",
    "prismjs": "^1.25.0",
    "sharp": "^0.29.2",
    "string-similarity": "^4.0.4",
    "vue-lazy-hydration": "^2.0.0-beta.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blagues-API/blagues-api.git"
  },
  "homepage": "https://www.blagues-api.fr",
  "bugs": {
    "url": "https://github.com/Blagues-API/blagues-api/issues"
  }
}
